class Banner:
    def __init__(self):
        self._description = "Welcome to the 'Iron Spirit' tournament in Mobile Legends: Bang Bang! Our banner reflects the energy and competitive spirit of the event, celebrating pride in its Ukrainian roots."
        self._logo = self.create_logo()
        self._characters = self.create_mlbb_heroes()
        self._rating_map = self.create_mlbb_detailed_rating_map()
        self._command_emblem = self.generate_esports_command_emblem()

    @property
    def description(self):
        return self._description

    @property
    def logo(self):
        return self._logo

    @property
    def characters(self):
        return self._characters

    @property
    def rating_map(self):
        return self._rating_map

    @property
    def command_emblem(self):
        return self._command_emblem

    # Останні методи класу не змінені...
